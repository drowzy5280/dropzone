{
  "name": "dropzone",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name init",
    "db:seed": "tsx prisma/seed.ts",
    "gen:secret": "node -e \"console.log(require('crypto').randomBytes(32).toString('base64'))\"",
    "env:print": "node -e \"require('fs').readFileSync('.env','utf8').split('\\n').forEach(l=>console.log(l))\"",
    "env:doctor": "node scripts/envDoctor.js",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.19.0",
    "@tanstack/react-query": "^5.56.0",
    "@vercel/og": "^0.8.5",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "ky": "^1.2.4",
    "lucide-react": "^0.552.0",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^2.12.7",
    "sonner": "^2.0.7",
    "stripe": "^16.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prisma": "^5.19.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  }
}
